{
  "name": "gltf-suspendable-loader",
  "version": "0.1.0",
  "description": "A suspendable loader for GLTF models.",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.esm.js",
  "types": "./dist/index.d.ts",
